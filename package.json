  "scripts": {
    "test:integration": "ENV_FILE=./config/.env.test jest --detectOpenHandles --forceExit",
    "test:unit": "ENV_FILE=./config/.env.test jest --detectOpenHandles --forceExit",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\", \"target\": \"es2016\" }' mocha -r ts-node/register 'src/test/**/*.ts'",
    "start:dev": "ts-node-dev --respawn --pretty --transpile-only src/server.ts",
    "start": "node src/server.js",
    "build": "tsc --strict false",
    "postinstall_": "npm run build"
  },
